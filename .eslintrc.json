{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        // "singleQuote": true,
        // "tabWidth": 2,
        // "useTabs": false,
        // "semi": true,
        // "bracketSpacing": true,
        // "arrowParens": "always",
        // "trailingComma": "none",
        // "printWidth": 80,
        // "proseWrap": "preserve",
        // "endOfLine": "lf"
      }
    ],
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },

  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
